        </div>
    </div>
</div>

<p>


</p>
<br />
        <table class="table table-hover">
            <thead>
                <tr>
                    <th>Restaurante</th>
                    <th>Comentario</th>
                </tr>
            </thead>

            @foreach (var Cliente in Model)
            {
                <tr>
                    <td>@Cliente.Restaurante</td>
                    <td>@Cliente.Comentario</td>
                </tr>
            }

        </table>
